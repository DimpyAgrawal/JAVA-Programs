class Cylinder{
    int height;
    int radius;
    double v;
    double a;
    
    public void setheight(int n){
        
        height=n;
    }
    
    public int getheight(){
        return height;
    }
    
    public void setradius(int i){
        radius = i;
    }
    
    public int getradius(){
        return radius;
    }
    
    public double area(){
        a=2*3.14*radius*height + 2*3.14*radius*radius;
        return a;
    }
    
    public double volume(){
        v=3.14*radius*radius*height;
       return v;
    }
    
    
}
public class Main
{
	public static void main(String[] args) {
	    Cylinder cyr = new Cylinder();
	    
	    cyr.setheight(5);
	    System.out.println("Height is : "+ cyr.getheight());
	    
	    cyr.setradius(5);
	    System.out.println("radius is : "+ cyr.getradius());
	    
	   
	    System.out.println("area is : "+ cyr.area());
	    

	    System.out.println("volume is :"+ cyr.volume());
	    
	    
		
	}
}
