class EkClass{
    int a;
    
    public int getA(){
        return a;
    }
    
    EkClass(int a){
        this.a = a;
    }
    
    public int returnone(){
        return 1;
    }
}
    
class SClass extends EkClass{
    SClass(int c){
        super(c);
        //System.out.println("I am a constructor ");
        
    }
}


public class Main
{
	public static void main(String[] args) {
    	EkClass e = new EkClass(4);
    	SClass d = new SClass(67);
    	System.out.println(e.getA());
	}

}
