class Tommy{
        public void fire(){
            System.out.println("Firing on the enemy");
        }
        public void run(){
            System.out.println("Running from the enemy");
        }
        public void hit(){
            System.out.println("Hitting the enemy");
        }
    }
    
    public class Main
    {
	public static void main(String[] args) {
	Tommy player = new Tommy();
	player.fire();
	player.run();
	player.hit();
		
	}
}
